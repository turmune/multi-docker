{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
   {
     "name": "client",
     "image": "turmune/multi-client",
     "hostname": "client",
     "essential": false,
     "memory": 128
   },
   {
     "name": "server",
     "image": "turmune/multi-server",
     "hostname": "api",
     "essential": false,
     "memory": 128     
   },
   {
     "name": "worker",
     "image": "turmune/multi-workers",
     "hostname": "worker",
     "essential": false,
     "memory": 128     
   },
   {
     "name": "nginx",
     "image": "turmune/multi-nginx",
     "hostname": "nginx",
     "essential": true,
     "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        } 
      ],
      "links": ["client", "server"],
      "memory": 128
   }
  ]
}